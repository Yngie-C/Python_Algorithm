    "def max_and_min(s):\n",
    "    numList = [int(num) for num in s.split(' ')]\n",
    "    maximum = max(numList)\n",
    "    minimum = min(numList)\n",
    "    return str(minimum)+\" \"+str(maximum)"
   ]
  }
