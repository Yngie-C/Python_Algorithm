    "def hate_same_number(arr):\n",
    "    arr2 = [arr[0]]\n",
    "    for idx in range(len(arr)-1):\n",
    "        if arr[idx] != arr[idx+1]:\n",
    "            arr2.append(arr[idx+1])\n",
    "    answer = arr2\n",
    "    return answer"
   ]
